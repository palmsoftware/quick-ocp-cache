{
  "description": "OCP versions to build CRC cache images for",
  "versions": [
    "4.18",
    "4.19",
    "4.20"
  ],
  "version_mappings": {
    "4.18": {
      "crc_version": "2.51.0",
      "description": "OpenShift 4.18 LTS",
      "release_url": "https://github.com/crc-org/crc/releases/tag/v2.51.0"
    },
    "4.19": {
      "crc_version": "2.54.0",
      "description": "OpenShift 4.19",
      "release_url": "https://github.com/crc-org/crc/releases/tag/v2.54.0"
    },
    "4.20": {
      "crc_version": "2.56.0",
      "description": "OpenShift 4.20 (latest)",
      "release_url": "https://github.com/crc-org/crc/releases/tag/v2.56.0"
    }
  },
  "registry": {
    "url": "quay.io",
    "image": "bapalm/quick-ocp-cache"
  },
  "architectures": [
    "amd64",
    "arm64"
  ],
  "notes": [
    "Add new OCP versions to the 'versions' array as they are released",
    "Update 'version_mappings' with the corresponding CRC version",
    "The workflow will build images for all versions in the 'versions' array",
    "Images are tagged as: quay.io/bapalm/quick-ocp-cache:<ocp_version>"
  ]
}

